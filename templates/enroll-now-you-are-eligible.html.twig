{#
  /**
    * @file
    * Default theme implementation for an item list.
    *
    * Available variables:
    *
    * @see template_preprocess_template_directory()
    *
    * @ingroup themeable
    */
#}
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
<div class="you-are-eligible">
  <div class="progress-row">
    <div class="progress">
      <div class="determinate" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width:60%">
      </div>
    </div>
  </div>
  {{ form.actions.previous }}
  <div class="you-are-eligible__text">
    {{ form.label }}
  </div>
  <div class="you-are-eligible__selection">
    {{ form.get_product }}
  </div>
  <div class="you-are-eligible__submit">
    {{ form.actions.submit }}
  </div>
</div>

{# Added popup message to alert the user to leave data session #}
{% include '/modules/custom/enroll_now/templates/alert-remove-data-session.html.twig' %}
