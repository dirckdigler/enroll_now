{#
  /**
   * @file
   * Default theme implementation for an item list.
   *
   * Available variables:
   *
   * @see template_preprocess_template_directory()
   *
   * @ingroup themeable
   */
#}
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
<div class="personal-info">
  <div class="progress-row">
    <div class="progress">
      <div class="determinate" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width:20%"></div>
    </div>
  </div>

  <div class="form-container">
    <div class="row">
      <div class="col">
        {{ form.label }}
      </div>
    </div>

    <div class="row form-element">
      <div class="my-insurance">
        <div class="col">
          {{ form.phone_number }}
        </div>

        {# Added tooltip functionality #}
        {% include '/modules/custom/enroll_now/templates/info-id-insurance.html.twig' %}

        <div class="col">
          {{ form.email }}
        </div>
      </div>
    </div>

    <div class="row form-element">
      <div class="col check-element">
        {{ form.check }}
      </div>
    </div>

    <div class="row form-element">
      <div class="col submit-form">
        {{ form.actions.submit }}
      </div>
    </div>

  </div>
</div>
{# Added modal personal data functionality #}
{% include '/modules/custom/enroll_now/templates/personal-data-modal.html.twig' %}
{# Added popup message to alert the user to leave data session #}
{% include '/modules/custom/enroll_now/templates/alert-remove-data-session.html.twig' %}
